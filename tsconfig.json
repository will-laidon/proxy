{
  "compilerOptions": {
      "lib": ["ES2019"],
      "module": "commonjs",
      "target": "ES2019",
      "esModuleInterop": true,
      "sourceMap": true,
      "inlineSources": true,
      "outDir": "./gen/srv/srv",
      "resolveJsonModule": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "preserveConstEnums": true,
      "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
